    fields:
      title: Demo: Calculate license cost
      context@text:
      owner__context: app
      owner_id: 0
      content@raw:
        {% set seat_rate = 30.00
          * (1 == prompt_discount_academic ? 0.75 : 1.0)
          * (1 == prompt_discount_renewal ? 0.80 : 1.0)
        %}
        Software licenses are $30.00 USD per seat-month.

        {% if seat_rate != 30.00 %}
        You qualify for the following discounts:
        {% if 1 == prompt_discount_renewal %}
         * Timely renewal (20% off)
        {% endif %}
        {% if 1 == prompt_discount_academic %}
         * Academic (25% off)
        {% endif %}

        Your discounted price is ${{seat_rate|number_format(2)}} USD per seat-month.

        {% endif %}
        For one year of priority support and software updates:
        {{prompt_seats}} seats * ${{seat_rate|number_format(2)}}/seat-month * 12 months = ${{(prompt_seats * seat_rate * 12)|number_format(2)}} USD

        Would you like me to send over an invoice?
      prompts_kata@text:
        text/prompt_seats:
          label: How many seats?
          default: 5

        checkbox/prompt_discount_academic:
          label: Academic discount?
          default@bool: no

        checkbox/prompt_discount_renewal:
          label: Timely renewal discount?
          default@bool: no
