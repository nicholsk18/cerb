workflow:
  name: cerb.demo.data
  version: 2024-10-12T00:00:00Z
  description: Realistic sample data for demonstration, testing, and development
  requirements:
    cerb_version: >=10.5 <11.0
    cerb_plugins: cerberusweb.core,

records:
  mail_transport/transport_null:
    fields:
      name: Demo Mail Transport
      extension_id: core.mail.transport.null
  address/address_demo_at_cerb_dot_example:
    fields:
      email: demo@cerb.example
      mail_transport_id: {{records.transport_null.id}}
  html_template/html_template_demo:
    fields:
      name: Demo HTML Layout
      owner__context: app
      owner_id: 0
      content@raw:
        <div id="body">
        {{message_body}}
        </div>

        <style type="text/css">
        #body {
          font-family: Arial, Verdana, sans-serif;
          font-size: 10pt;
        }

        a {
          color: black;
        }

        blockquote {
          color: rgb(0, 128, 255);
          font-style: italic;
          margin-left: 0px;
          border-left: 1px solid rgb(0, 128, 255);
          padding-left: 5px;
        }

        blockquote a {
          color: rgb(0, 128, 255);
        }
        </style>
  email_signature/signature_demo:
    fields:
      name: Demo Signature
      owner__context: app
      owner_id@int: 0
      signature@raw:
        --
        {{full_name}}{% if title %}, {{title}}{% endif %}

        Cerb: Enterprise communication and workflow automation <https://cerb.ai/>
      signature_html@raw:
        --
        {{full_name}}{% if title %}, {{title}}{% endif %}

        **[Cerb](https://cerb.ai/)** - Enterprise communication and workflow automation
