        columns@list:
          c_email_id
          c_updated
          c_country
          c_phone
          c_website
  workspace_widget/widget_clients_contacts:
    fields:
      label: Contacts
      tab_id: {{records.tab_clients.id}}
      extension_id: core.workspace.widget.sheet
      pos@int: 5
      width_units@int: 2
      zone: content
      params:
        data_query@text:
          type:sample.records
          records:(help:())
          format:dictionaries
        cache_secs@text:
        placeholder_simulator_kata@text:
        sheet_kata@text:
          layout:
            style: fieldsets
            headings@bool: no
            paging@bool: no
