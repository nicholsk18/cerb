{
  "package": {
    "name": "Connected Service: Anthropic",
    "revision": 1,
    "requires": {
      "cerb_version": "10.4.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Anthropic",
      "uri": "cerb_connected_service_anthropic",
      "description": "Integration with Anthropic",
      "instructions": "# Instructions\r\n\r\n* Enter your API key from <https://console.anthropic.com/settings/keys>",
      "point": "connected_service",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [],
      "prompts": [
        {
          "type": "text",
          "label": "API Key",
          "key": "prompt_api_key",
          "params": {
            "default": "",
            "placeholder": "(paste your API Key)"
          }
        },
        {
          "type": "text",
          "label": "Alias",
          "key": "prompt_name",
          "params": {
            "default": "anthropic",
            "placeholder": "(enter a URI, a-z,0-9,_)"
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "service_anthropic",
      "_context": "connected_service",
      "name": "Anthropic",
      "extension_id": "cerb.service.provider.api_key",
      "params": {
        "api_base_url": "https://api.anthropic.com/",
        "api_key_name": "x-api-key",
        "api_key_location": "header"
      }
    },
    {
      "uid": "account_anthropic",
      "_context": "connected_account",
      "name": "Anthropic",
      "service_id": "{{{uid.service_anthropic}}}",
      "owner__context": "cerberusweb.contexts.app",
      "owner_id": "0",
      "uri": "{{{prompt_name}}}",
      "params": {
        "api_key": "{{{prompt_api_key}}}"
      }
    }
  ]
}