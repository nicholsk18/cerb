{
	"package": {
		"name": "Installer Base Package",
		"revision": 1,
		"requires": {
			"cerb_version": "10.5.0"
		},
		"configure": {
			"prompts": [
				{
					"type": "text",
					"label": "Admin first name:",
					"key": "admin_name_first",
					"params": {
					}
				},
				{
					"type": "text",
					"label": "Admin last name:",
					"key": "admin_name_last",
					"params": {
					}
				},
				{
					"type": "text",
					"label": "Admin email:",
					"key": "admin_email",
					"params": {
					}
				},
				{
					"type": "text",
					"label": "Admin password:",
					"key": "admin_password",
					"params": {
					}
				},
				{
					"type": "text",
					"label": "Admin timezone:",
					"key": "admin_timezone",
					"params": {
					}
				}
			],
			"placeholders": [

			]
		}
	},
	"records": [
		{
			"uid": "address_admin_email",
			"_context": "address",
			"email": "{{{admin_email}}}"
		},
		{
			"uid": "calendar_office_hours",
			"_context": "calendar",
			"name": "Office Hours",
			"owner__context": "app",
			"owner_id": 0,
			"params": {
				"manual_disabled": "0",
				"sync_enabled": "1",
				"start_on_mon": "1",
				"hide_start_time": "1",
				"color_available": "#A0D95B",
				"color_busy": "#C8C8C8",
				"series": [

				]
			}
		},
		{
			"uid": "calendar_event_work",
			"_context": "calendar_recurring_event",
			"calendar_id": "{{{uid.calendar_office_hours}}}",
			"name": "Work (8a-6p)",
			"is_available": 1,
			"tz": "America/Los_Angeles",
			"event_start": "08:00:00",
			"event_end": "17:59:59",
			"recur_start": 0,
			"recur_end": 0,
			"patterns": "weekdays"
		},
		{
			"uid": "calendar_admin",
			"_context": "calendar",
			"name": "{{{admin_name_first}}}'s Calendar",
			"owner__context": "worker",
			"owner_id": "{{{uid.worker_admin}}}",
			"params": {
				"manual_disabled": "0",
				"sync_enabled": "1",
				"start_on_mon": "1",
				"hide_start_time": "1",
				"color_available": "#A0D95B",
				"color_busy": "#C8C8C8",
				"series": [
					{
						"datasource": "calendar.datasource.calendar",
						"sync_calendar_id": "{{{uid.calendar_office_hours}}}"
					}
				]
			}
		},
		{
			"uid": "worker_admin",
			"_context": "worker",
			"first_name": "{{{admin_name_first}}}",
			"last_name": "{{{admin_name_last}}}",
			"email_id": "{{{uid.address_admin_email}}}",
			"title": "Administrator",
			"at_mention_name": "{{{admin_name_first}}}",
			"is_superuser": 1,
			"is_disabled": 0,
			"timezone": "{{{admin_timezone}}}",
			"time_format": "D, d M Y h:i a",
			"password": "{{{admin_password}}}",
			"language": "en_US",
			"calendar_id": "{{{uid.calendar_admin}}}"
		},
		{
			"uid": "group_support",
			"_context": "group",
			"name": "Support",
			"is_default": 1,
			"is_private": 0,
			"members": {
				"manager": "{{{uid.worker_admin}}}"
			},
			"image": "data:image/png;base64,"
		},
		{
			"uid": "bucket_support_inbox",
			"_context": "bucket",
			"name": "Inbox",
			"is_default": "1",
			"group_id": "{{{uid.group_support}}}"
		},
		{
			"uid": "org_wgm",
			"_context": "org",
			"name": "Webgroup Media, LLC.",
			"street": "440 N Barranca Ave #2048",
			"city": "Covina",
			"postal": "91723",
			"province": "California",
			"country": "United States",
			"phone": "+1-714-671-9090",
			"website": "https://cerb.ai"
		},
		{
			"uid": "address_team_at_cerb_dot_ai",
			"_context": "address",
			"email": "team@cerb.ai",
			"org_id": "{{{uid.org_wgm}}}"
		},
		{
			"uid": "ticket_001",
			"_context": "ticket",
			"group_id": "{{{uid.group_support}}}",
			"subject": "Welcome to Cerb!",
			"importance": 100,
			"status": "open",
			"org_id": "{{{uid.org_wgm}}}",
			"spam_score": 0.0001,
			"spam_training": "N",
			"participants": "team@cerb.ai"
		},
		{
			"uid": "message_001_001",
			"_context": "message",
			"ticket_id": "{{{uid.ticket_001}}}",
			"sender_id": "{{{uid.address_team_at_cerb_dot_ai}}}",
			"response_time": 0,
			"is_outgoing": 0,
			"hash_header_message_id": "{{{'<welcome@cerb.example>'|sha1}}}",
			"headers": "From: Cerb Team <team@cerb.ai>\nSubject: Welcome to Cerb!\nX-Mailer: Cerb\nContent-Type: text/plain; charset=utf-8\nMessage-Id: <welcome@cerb.example>\n",
			"content": "Welcome to Cerb!"
		},
		{
			"uid": "role_everyone",
			"_context": "role",
			"name": "Everyone",
			"privs_mode": "all",
			"member_query_worker": "isDisabled:n",
			"editor_query_worker": "isAdmin:y isDisabled:n",
			"reader_query_worker": "isDisabled:n"
		},
		{
			"uid": "role_admins",
			"_context": "role",
			"name": "Admins",
			"privs_mode": "all",
			"member_query_worker": "isAdmin:y isDisabled:n",
			"editor_query_worker": "isAdmin:y isDisabled:n",
			"reader_query_worker": "isDisabled:n"
		},
		{
			"uid": "saved_search_tickets_unassigned",
			"_context": "saved_search",
			"name": "Unassigned Tickets",
			"context": "ticket",
			"owner__context": "app",
			"owner_id": 0,
			"query": "owner.id:0 status:o",
			"tag": "unassigned-tickets"
		},
		{
			"uid": "saved_search_tasks_open",
			"_context": "saved_search",
			"name": "Open Tasks",
			"context": "task",
			"owner__context": "app",
			"owner_id": 0,
			"query": "status:o",
			"tag": "open-tasks"
		},
		{
			"uid": "saved_search_tasks_unassigned",
			"_context": "saved_search",
			"name": "Unassigned Tasks",
			"context": "task",
			"owner__context": "app",
			"owner_id": 0,
			"query": "owner.id:0 status:o",
			"tag": "unassigned-tasks"
		}
	]
}